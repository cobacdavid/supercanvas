from supercanvas import

w = beginMagicTk()
c = supercanvas(w, bg="white")

c.setViewX(-1, 1)
c.setViewY(-1, 1)

import math

c.step=.1

for i in range(0, 5):
    f = lambda x: math.cos(x) ** i
    f = lambda x: math.cos(x) ** i
    id = c.drawParam(f, g)
    c.update
    c.after(10)
    c.delete(id)

endMagicTk(w)
